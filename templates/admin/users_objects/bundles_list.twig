<table class="table table-responsive table-striped table-hover table-condensed table-bordered">
    <thead>
        <tr>
            <th>Paslaugų paketas</th>
            <th>{{ "created_at"|trans }}</th>
            <th>{{ "active_to"|trans }}</th>
        </tr>
    </thead>
    <tbody>
        {% for userObjectBundle in field.value %}
            <tr>
                <td>
                    <a class="list-group-item" href="/admin?crudAction=detail&crudControllerFqcn=App\Controller\Admin\UsersObjectsServicesBundlesCrudController&entityId={{ userObjectBundle.id }}">
                        #{{ userObjectBundle.id }}
                    </a>
                </td>
                <td>{{ userObjectBundle.created_at|date('Y-m-d H:i:s') }}</td>
                <td>{{ userObjectBundle.active_to|date('Y-m-d H:i:s') }}</td>
            </tr>
        {% else %}
            <tr class="text-center">
                <td colspan="3">Objektas neturi paslaugų paketų</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
