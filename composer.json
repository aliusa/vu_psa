{
    "name": "alius/psa",
    "description": "psa",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-calendar": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "^1.5",
        "composer/package-versions-deprecated": "^1.11.99",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "easycorp/easyadmin-bundle": "^4.24",
        "elasticsearch/elasticsearch": "^8.17",
        "friendsofsymfony/ckeditor-bundle": "^2.6",
        "friendsofsymfony/user-bundle": "^4.0",
        "gedmo/doctrine-extensions": "^3.17",
        "google/auth": "^1.45",
        "guzzlehttp/guzzle": "^7.9",
        "halaxa/json-machine": "^1.2.0",
        "knplabs/knp-components": "^5.1",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpoffice/phpspreadsheet": "^1.23",
        "symfony/asset": "^7.2",
        "symfony/console": "^7.2",
        "symfony/dotenv": "^7.2",
        "symfony/flex": "^1.17",
        "symfony/form": "^7.2",
        "symfony/framework-bundle": "^7.2",
        "symfony/http-client": "^7.2",
        "symfony/http-foundation": "^7.2",
        "symfony/lock": "^7.2",
        "symfony/mailer": "^7.2",
        "symfony/monolog-bundle": "^v3.10",
        "symfony/password-hasher": "^7.2",
        "symfony/property-access": "^7.2",
        "symfony/property-info": "^7.2",
        "symfony/proxy-manager-bridge": "^6.4",
        "symfony/runtime": "^7.2",
        "symfony/security-bundle": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/translation": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/validator": "^7.2",
        "symfony/yaml": "^7.2",
        "symfonycasts/sass-bundle": "^0.8.2",
        "twig/extra-bundle": "^3.18",
        "twig/intl-extra": "^3.20",
        "twig/string-extra": "^3.20",
        "twig/twig": "^3.18"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "symfony/maker-bundle": "^1.61",
        "symfony/stopwatch": "~7.0",
        "symfony/web-profiler-bundle": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "src-dir": "src/App"
    }
}
