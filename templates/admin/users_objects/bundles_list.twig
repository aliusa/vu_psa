<table class="table table-responsive table-striped table-hover table-condensed table-bordered">
    <thead>
        <tr>
            <th>Paslaugų paketas</th>
            <th>Paslaugos</th>
            <th>Sąskaitos</th>
            <th>{{ "created_at"|trans }}</th>
            <th>{{ "active_to"|trans }}</th>
        </tr>
    </thead>
    <tbody>
        {% for userObjectBundle in field.value %}
            <tr data-href="/admin?crudAction=detail&crudControllerFqcn=App\Controller\Admin\UsersObjectsServicesBundlesCrudController&entityId={{ userObjectBundle.id }}">
                <td>#{{ userObjectBundle.id }}</td>
                <td>{{ userObjectBundle.getUsersObjectsServices().count() }}</td>
                <td>{{ userObjectBundle.getInvoices().count() }}</td>
                <td>{{ userObjectBundle.created_at|date('Y-m-d H:i:s') }}</td>
                <td class="{% if userObjectBundle.isActive() %}text-success{% else %}text-danger{% endif %}">{{ userObjectBundle.active_to|date('Y-m-d') }}</td>
            </tr>
        {% else %}
            <tr class="text-center">
                <td colspan="20">Objektas neturi paslaugų paketų</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
