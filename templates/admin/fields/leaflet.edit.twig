{% block leaflet_widget %}
    <div class="input-group">
        {# Symfony teisingai i≈°ves ID, NAME, VALUE ir pan. #}
        <input class="form-control leaflet-value"
               type="text"
               id="{{ id }}"
               name="{{ full_name }}"
               value="{{ value|e }}"
        />
    </div>
    <div class="input-group">
        <small class="form-text text-muted">lat,lng,zoom</small>
    </div>
    <div class="leaflet-map mt-3"
         style="width: 800px; height: 400px; cursor: pointer;"
         data-coordinates="{{ value|e }}">
    </div>
{% endblock %}
